{
  "name": "codecolab",
  "scripts": {
    "test": "npm run eslint && npm run stylelint",
    "build": "webpack",
    "watch": "webpack --watch",
    "eslint": "eslint index.js server.js",
    "stylelint": "stylelint index.scss",
    "prepublishOnly": "npm run test && rm ./dist/* && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@osjs/dev-meta": "^2.1.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "stylelint": {
    "extends": "@osjs/stylelint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "osjs": {
    "type": "package"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  }
}
